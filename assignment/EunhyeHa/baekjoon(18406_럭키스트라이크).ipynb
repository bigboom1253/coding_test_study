    "\n",
    "for i in range(splN):\n",
    "    num_0 += int(str_N[i])\n",
    "    num_1 += int(str_N[i+splN])\n",
    "    \n",
    "if num_0 == num_1:\n",
    "    print('LUCKY')\n",
    "else:\n",
    "    print('READY')"
