   "source": [
    "S = input()\n",
    "\n",
    "count_0 = 0\n",
    "count_1 = 0\n",
    "\n",
    "if S[0] == '1':\n",
    "    count_0 += 1\n",
    "else:\n",
    "    count_1 += 1\n",
    "\n",
    "for i in range(len(S)-1):\n",
    "    if S[i] != S[i+1]:\n",
    "        if S[i+1] == '1':\n",
    "            count_0 += 1\n",
    "        else:\n",
    "            count_1 += 1\n",
    "\n",
    "print(min(count_0, count_1))"
   ]
